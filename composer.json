{
    "name": "twigger/unioncloud",
    "description": "An API Wrapper for the NUS UnionCloud platform",
    "type": "library",
    "license": "GPL-3.0-or-later",
    "authors": [
        {
            "name": "Toby Twigger",
            "email": "tt15951@bristol.ac.uk"
        }
    ],
    "require": {
        "nesbot/carbon": "^1.26",
        "guzzlehttp/guzzle": "^6.3",
        "ext-json": "^1.6",
        "illuminate/support": "5.6.*|5.7.*|5.8.*"
    },
    "minimum-stability": "dev",
    "autoload": {
         "psr-4": {
             "Twigger\\UnionCloud\\API\\": "src/"
         }
    },
    "autoload-dev": {
        "psr-4": {
            "Twigger\\UnionCloud\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^7",
        "vlucas/phpdotenv": "^2.5@dev"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Twigger\\UnionCloud\\API\\UnionCloudServiceProvider"
            ]
        }
    }
}
